  "dependencies": {
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@ffprobe-installer/ffprobe": "^1.4.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/react-fontawesome": "^0.1.15",
    "@mui/material": "^5.10.3",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "axios": "^0.24.0",
    "axios-retry": "^3.2.4",
    "concurrently": "^7.3.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "env-cmd": "^10.1.0",
    "ffmpeg": "^0.0.4",
    "get-video-duration": "^3.0.2",
    "joi": "^17.4.0",
    "joi-objectid": "^4.0.2",
    "knex": "^0.95.14",
    "mongoose": "^5.12.13",
    "node-fetch": "^2.6.1",
    "node-ffprobe-installer": "^1.2.3",
    "pg": "^8.7.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "sqlite3": "^5.0.2",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start:frontend": "env-cmd -f src/.env react-scripts start",
    "start:backend": "nodemon ANALYSIS=$npm_config_analysis server.js",
    "start:all": "concurrently \"yarn start:backend\" \"yarn start:frontend",
    "start:all-prod": "concurrently \"serve -s build\" \"yarn start:backend",
    "electron:serve": "concurrently -k \"cross-env BROWSER=none yarn start:frontend\" \"yarn electron:start\"",
    "electron:start": "wait-on tcp:3000 && electron .",
    "build": "env-cmd -f src/.env react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "electron": "^20.1.1",
    "eslint": "^7.28.0",
    "wait-on": "^6.0.1"
  }
}
